{
  "kind": "collectionType",
  "collectionName": "wishlists",
  "info": {
    "singularName": "wishlist",
    "pluralName": "wishlists",
    "displayName": "Wishlist",
    "description": "User's wishlist of desired products"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "createdAt": {
      "type": "datetime"
    },
    "updatedAt": {
      "type": "datetime"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "wishlists"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "wishlist"
    },
    "wishlist_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wishlist-item.wishlist-item",
      "mappedBy": "wishlist"
    }
  }
}
