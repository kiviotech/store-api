{
  "kind": "collectionType",
  "collectionName": "cart_items",
  "info": {
    "singularName": "cart-item",
    "pluralName": "cart-items",
    "displayName": "Cart Item",
    "description": "Individual items in a user's shopping cart"
  },
  "options": {
    "draftAndPublish": false,
    "comment": ""
  },
  "attributes": {
    "cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cart.cart",
      "inversedBy": "cartItems"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product"
    },
    "productSku": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-sku.product-sku"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "createdAt": {
      "type": "datetime"
    },
    "updatedAt": {
      "type": "datetime"
    }
  }
}
